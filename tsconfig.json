{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/layouts/*": ["./src/layouts/*"],
      "@/svg/*": ["./src/assets/svg/*"],
      "@/db/*": ["./src/db/*"],
      "@/types": ["./src/types.ts"],
      "@/lib/*": ["./src/lib/*"]
    }
  }
}
