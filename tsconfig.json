{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client"],
    "baseUrl": ".",
    "lib": ["es6", "dom", "dom.iterable", "esnext"],
    "paths": {
      "@/*": ["./src/*"],
      "@/actions/*": ["./src/actions/*"],
      "@/layouts/*": ["./src/layouts/*"],
      "@/svg/*": ["./src/assets/svg/*"],
      "@/db/*": ["./src/db/*"],
      "@/types": ["./src/types.ts"],
      "@/lib/*": ["./src/lib/*"],
      "@/middleware/*": ["./src/middleware/*"]
    }
  }
}
